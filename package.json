{
  "name": "plateforme-evaluation",
  "version": "1.0.0",
  "description": "Plateforme d'évaluation automatisée des exercices de bases de données",
  "scripts": {
    "start:backend": "cd backend && uvicorn app.main:app --reload",
    "start:frontend": "cd frontend && npm start",
    "dev": "concurrently \"npm run start:backend\" \"npm run start:frontend\""
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
} 